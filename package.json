{
  "name": "elasticpath-graphql-server",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server playground",
    "dev:server": "nodemon -e ts,graphql -x ts-node -r dotenv/config src",
    "playground": "graphql playground",
    "deploy": "now --public --dotenv .env.production && now alias && now rm --yes --safe moltinql",
    "build": "rimraf dist && tsc",
    "start": "node dist"
  },
  "dependencies": {
    "@moltin/sdk": "^6.6.0",
    "dataloader": "^2.0.0",
    "node-fetch": "^2.0.0",
    "apollo-datasource": "^0.1.3",
    "apollo-datasource-rest": "^0.1.5",
    "apollo-server": "^2.15.0",
    "apollo-server-testing": "^2.15.0",
    "dotenv": "^6.2.0",
    "graphql": "^14.6.0",
    "isemail": "^3.1.3",
    "mime": "^2.4.4",
    "nodemon": "^1.19.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^10.17.59",
    "@types/node-fetch": "^2.5.10",
    "dotenv": "^8.2.0",
    "graphql-cli": "^4.1.0",
    "nodemon": "^2.0.7",
    "now": "^21.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "apollo": "^2.32.5",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "jest": "^25.0.0",
    "nock": "^10.0.2",
    "node-fetch": "^2.2.1",
    "now": "^12.1.3"
  },
  "now": {
    "alias": "elasticpath-graphql-server"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=12 <15"
  }
}
